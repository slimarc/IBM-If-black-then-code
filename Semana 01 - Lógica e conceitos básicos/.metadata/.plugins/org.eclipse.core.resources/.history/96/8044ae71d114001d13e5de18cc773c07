import java.util.Scanner;

	public class EscolhendoCarros {
		public static void main(String[] args) {
			Scanner input = new Scanner(System.in);
			String[] carros = new String[10];
			int pos;
			
			System.out.println("Insira os dez carros: ");
			for(pos=0; pos<=9; pos++) {
				carros[pos] = input.nextLine();
				System.out.println("Os carros inseridos foram: "+ carros[pos]);
			}
			
			
			input.close();
		}
	}
	

